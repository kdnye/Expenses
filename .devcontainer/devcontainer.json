{
  "name": "FSI Expenses Dev",
  "dockerComposeFile": ["../compose.yaml"],
  "service": "api",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "forwardPorts": [3000, 5432],
  "postCreateCommand": "echo 'Run npm install manually before executing frontend tests.'",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true,
        "files.exclude": {
          "**/.idea": true,
          "**/.DS_Store": true
        }
      }
    }
  }
}
